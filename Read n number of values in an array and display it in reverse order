#include<stdio.h>
int main() {
    int n, i, a[10];
    printf("Input the number of elements to store in the array :");
    scanf("%d",&n);
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }
    printf("the order of the elements are:\n");

    for (i = 0; i < n; i++) {
        printf("%d ", a[i]);
    }
    printf("\n\nreverse order of the elements are:\n");
    for (i = n - 1; i >= 0; i--) {
        printf("%d ", a[i]);
        
    }

    return 0;
}
