#include<stdio.h>
int main(){
    int arr[]={1,2,3,4,5,6,7};
    int n=sizeof(arr)/sizeof(arr[0]),i;
    int even_count=0,odd_count=0;
    for(i=0;i<n;i++)
        if(arr[i]%2==0)
            even_count++;
    else
       odd_count++;
    printf("even count is:%d\n",even_count);
    printf("odd count is:%d\n",odd_count);
}
