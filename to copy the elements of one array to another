#include<stdio.h>
int main()
{
    
     int a[5]={1,2,5,6,7},n=5;
     int b[n],i;
     for(i=0;i<n;i++){
         b[i]=a[i];
     }
    printf("the first array of elements\n:");
    for(i=0;i<n;i++){
        printf("%d",a[i]);
    }
    printf("the second array of elements\n:");
    for(i=0;i<n;i++){
        printf("%d",b[i]);
    }
}
